<region>
sample=..\libs\Other\Samples/Timpani/timpani-roll-G2-PB.wav
lokey=f#2 hikey=g#2 pitch_keycenter=g2

<region>
sample=..\libs\Other\Samples/Timpani/timpani-roll-A2-PB.wav
lokey=a2 hikey=a#2 pitch_keycenter=a2

<region>
sample=..\libs\Other\Samples/Timpani/timpani-roll-C3-PB.wav
lokey=b2 hikey=c#3 pitch_keycenter=c3

<region>
sample=..\libs\Other\Samples/Timpani/timpani-roll-D3-PB.wav
lokey=d3 hikey=d#3 pitch_keycenter=d3

<region>
sample=..\libs\Other\Samples/Timpani/timpani-roll-E3-PB.wav
lokey=e3 hikey=f3 pitch_keycenter=e3

<region>
sample=..\libs\Other\Samples/Timpani/timpani-roll-G3-PB.wav
