//==============================================================
// Percussion - Timpani - Key Switches
//
//   This is a combination of
//
//     timpani-hit.sfz
//     timpani-roll.sfz
//
//   ... to create a Key Switchable patch
//
//     key switch c5  = Hit
//     key switch d5  = Roll
//
//   by Paul Battersby - http://virtualplaying.com
//
//==============================================================

//==============================================================
//  Percussion - Timpani Hit
//
//   timpani.sfz from No Budget Orchestra - https://nobudgetorchestra.net
//
//   modified by Paul Battersby - http://virtualplaying.com
//
//==============================================================

 // ----------------- Right Hand -----------------
<group>
sw_default=c5  // hit by default
sw_lokey=c5
sw_hikey=d5
sw_last=c5
sw_label=C5 Hit
// randomize like a real player
ampeg_attack=0.03
offset_random=200
pitch_random=5
amp_random=1
delay_random=0.04
group_label=right_hand
group=1
seq_position=1
seq_length=2

ampeg_sustain=0 ampeg_hold=0 ampeg_decay=20 ampeg_release=3

<region>
sample=..\libs\NoBudgetOrch\Timpani\C2-PB.wav
volume=4

lokey=c2 hikey=c2 pitch_keycenter=c2

