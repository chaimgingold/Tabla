//==============================================================
// Bass Section - Sustain
//
//   Strings - Basses Sustain.sfz from Sonatina Symphonic Orchestra - http://sso.mattiaswestlund.net
//
//   modified by Paul Battersby - http://virtualplaying.com
//
//==============================================================

// ------------------------------
//  Sonatina Symphonic Orchestra
// ------------------------------
//        Basses Sustain
// ------------------------------

<group>
ampeg_attackcc1=0.5 // mod wheel slows attack
// randomize like a real player(PB)
offset_random=200
pitch_random=5
amp_random=1


ampeg_attack=0.03
ampeg_vel2attack=-0.300
ampeg_release=1.25

<region>
sample=..\libs\SSO\Samples\Basses\basses-sus-c1.wav
pan=10
volume=1

lokey=c1
hikey=c#1
pitch_keycenter=c1
tune=6

